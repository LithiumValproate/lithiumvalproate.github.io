#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules/postcss-cli/node_modules:/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules:/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules/postcss-cli/node_modules:/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules:/Users/kasugano/Documents/matsu_rip/nuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules/postcss-cli/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/postcss-cli@11.0.1_jiti@2.4.2_postcss@8.5.3/node_modules/postcss-cli/index.js" "$@"
fi
